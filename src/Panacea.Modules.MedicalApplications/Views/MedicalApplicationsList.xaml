<UserControl
    x:Class="Panacea.Modules.MedicalApplications.Views.MedicalApplicationsList"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:controls="clr-namespace:Panacea.ContentControls;assembly=Panacea.ContentControls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Panacea.Modules.MedicalApplications.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <controls:LazyLoadingTabControl
        Title="Medical"
        IsSearchable="False"
        ItemProvider="{Binding Provider}"
        OpenItemCommand="{Binding ItemClickCommand}"
        Color="#962037" />
</UserControl>
